{
  "call": "helloworld.Greeter.SayHello",
  "total": 10,
  "concurrency": 2,
  "metadata": {
    "trace_id": "{{.RequestNumber}}",
    "timestamp": "{{.TimestampUnix}}"
  },
  "data": {
    "name": "{{.WorkerID}}"
  },
  "max-duration": "10s",
  "host": "greeter-server.default.svc.cluster.local:50051",
  "insecure": true,
  "async": true,
  "format": "html",
  "output": "/results"
}