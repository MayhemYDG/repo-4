{
    "env": {
        "commonjs": true,
        "es6": true,
        "jasmine": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": [
        "license-header"
    ],
    "rules": {
        "license-header/header": [
            "error",
            "./license-header.js"
        ]
    }
}
